{
  "name": "angularjs-dashboard",
  "version": "1.0.0",
  "description": "A simple dashboard built using AngularJS",
  "scripts": {
    "start": "http-server ./src -a localhost -p 3000 -c-1",
    "postinstall": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-*,bootstrap/dist}/**/*\" src/lib -C"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/indratanaya/angularjs-dashboard.git"
  },
  "keywords": [
    "AngularJS",
    "Dashboard"
  ],
  "author": "Indra Tanaya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/indratanaya/angularjs-dashboard/issues"
  },
  "homepage": "https://github.com/indratanaya/angularjs-dashboard#readme",
  "dependencies": {
    "angular": "^1.8.3",
    "angular-route": "^1.8.3",
    "bootstrap": "^5.3.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "http-server": "^14.1.1"
  }
}
